<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.rockwellcollins.com/Spear" 
       class = "com.rockwellcollins.spear.SpearPackage"
       genModel = "model/generated/Spear.genmodel" /> 
	
  </extension>
  <extension
        id="spear_product"
        point="org.eclipse.core.runtime.products">
     <product
           application="org.eclipse.ui.ide.workbench"
           name="Spear">
        <property
              name="windowImages"
              value="branding/1429142139_weapons_spear-16.png,branding/1429142164_weapons_spear-32.png,branding/1429142171_weapons_spear-48.png,branding/1429142412_weapons_spear-64.png,branding/1429142344_weapons_spear-128.png,branding/1429142348_weapons_spear-256.png">
        </property>
        <property
              name="aboutText"
              value="SpeAR: A Tool for Prototyping and Analyzing Requirements">
        </property>
        <property
              name="aboutImage"
              value="branding/1429142344_weapons_spear-128.png">
        </property>
        <property
              name="appName"
              value="Spear">
        </property>
     </product>
  </extension>

</plugin>
